    <!-- Intro Page 2 -->
    <string name="intro_page2_title">एआई मॉडल की नई पीढ़ी</string>
    <string name="intro_page2_subtitle">हम आपको दुनिया के सर्वश्रेष्ठ मॉडल प्रदान करते हैं। पहचान के हमारे तीन स्तरों में से चुनें, प्रत्येक बढ़ती विश्लेषणात्मक गहराई प्रदान करता है।</string>
    <string name="intro_page2_quick_title">त्वरित</string>
    <string name="intro_page2_quick_description">तेज और कुशल पहचान।</string>
    <string name="intro_page2_standard_title">मानक</string>
    <string name="intro_page2_standard_description">सटीकता और गति के बीच संतुलन।</string>
    <string name="intro_page2_precise_title">अत्यंत सटीक</string>
    <string name="intro_page2_precise_description">कठिन मामलों के लिए गहन विश्लेषण।</string>
    <string name="intro_page2_button_next">अगला</string>

    <!-- Intro Page 3 -->
    <string name="intro_page3_title">तेज़ और सहज</string>
    <string name="intro_page3_description">सेकंडों में उत्तर पाने के लिए बस एक तस्वीर लें</string>
    <string name="intro_page3_button_next">अगला</string>

    <!-- Intro Page 4 -->
    <string name="intro_page4_title">हर जगह आपके साथ</string>
    <string name="intro_page4_description">चाहे टहलने पर हों या यात्रा पर, आप जल्द ही अपने आस-पास के सभी प्रकृति को जान और पहचान पाएंगे</string>
    <string name="intro_page4_button_next">अगला</string>

    <!-- Intro Page 5 -->
    <string name="intro_page5_title">अंतिम चरण</string>
    <string name="intro_page5_description">एप्लिकेशन का उपयोग करने के लिए, आपको अब Google से कनेक्ट होना चाहिए और आवश्यक अनुमतियों को स्वीकार करना चाहिए।</string>
    <string name="intro_page5_button_login">लॉग इन</string>

    <!-- Onboarding Fragment (Permissions) -->
    <string name="permission_camera_title">कैमरा अनुमति</string>
    <string name="permission_camera_description">इस एप्लिकेशन को पौधों और जानवरों की तस्वीरें लेने और उनका विश्लेषण करने के लिए आपके कैमरे तक पहुंच की आवश्यकता है।</string>
    <string name="permission_location_title">स्थान अनुमति</string>
    <string name="permission_location_description">आपके स्थान तक पहुंच आपके क्षेत्र के आधार पर प्रजाति पहचान की सटीकता में सुधार करती है।</string>
